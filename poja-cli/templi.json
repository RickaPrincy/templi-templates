{
  "scripts": {
    "after": [
      "chmod +x {{TEMPLI_OUTPUT_FOLDER}}/poja-cli.sh",
      "{{TEMPLI_OUTPUT_FOLDER}}/poja-cli.sh"
    ],
    "before": []
  },
  "excludes": [
    "templi.json"
  ],
  "keys": [
    {
      "name": "APP_NAME",
      "label": "App name",
      "type": "INPUT",
      "required": true
    },
    {
      "name": "AWS_REGION",
      "label": "Aws region",
      "type": "INPUT",
      "required": true,
      "default": "eu-west-3"
    },
    {
      "name": "PACKAGE_FULL_NAME",
      "label": "Package full name",
      "type": "INPUT",
      "required": true
    },
    {
      "name": "SG_ID",
      "label": "Security groupId",
      "type": "INPUT",
      "required": true,
      "default": "/hei/nat/poja/sg/id"
    },
    {
      "name": "SUBNET1_ID",
      "label": "Subnet1 id",
      "type": "INPUT",
      "required": true,
      "default": "/hei/nat/poja/subnet/private1/id"
    },
    {
      "name": "SUBNET2_ID",
      "label": "Subnet2 id",
      "type": "INPUT",
      "required": true,
      "default": "/hei/nat/poja/subnet/private2/id"
    }
  ]
}
